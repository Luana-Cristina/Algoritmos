algoritmo "semnome"
var
  valor, salto, cont: inteiro
inicio

    Escreval ("Quer contar até quanto: ")
    leia(valor)
     Escreval ("Qual será o valor do salto: ")
    leia(salto)
     cont <- 0
      Enquanto (cont <= valor) faca
       escreval (cont)
       cont <- cont + salto
       FimEnquanto
    
     Escreval ("Terminei de contar")

fimalgoritmo